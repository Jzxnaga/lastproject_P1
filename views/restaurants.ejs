<%- include('partials/header') %>
<%- include('partials/navbar') %>

<div class="container mt-5">
    <h2>Restautant</h2>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th>id</th>
                <th>name</th>
                <th>owner</th>
                <th>email</th>
                <th>review</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(el => {%>
            <tr>
                <td><%= el.id %></td>
                <td><%= el.name %></td>
                <td><%= el.owner %></td>
                <td><%= el.email %></td>
                <td> 
                    <a href="/restaurant/<%= el.id%> "><button>Reviews</button></a>
                    <a href=""><button>Delete</button></a>
                </td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>



<%- include('partials/footer') %>