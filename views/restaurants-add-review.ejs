<%- include('partials/header') %>
<%- include('partials/navbar') %>

<div class="container mt-5">
    <h2>Restautant</h2>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th>id</th>
                <th>name</th>
                <th>owner</th>
                <th>email</th>
                <th>review</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(element => {%>
            <tr>
                <td><%= element.id %></td>
                <td><%= element.name %></td>
                <td><%= element.owner %></td>
                <td><%= element.email %></td>
                <td> <a href=""><button>Edit</button></a>|
                    <a href=""><button>Delete</button></a></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>



<%- include('partials/footer') %>